14- Crie um algoritmo que leia três números e exibe o maior deles.

ALGORITMO "EX14"
	VAR
		N1, N2, N3, MN : REAL
INICIO
	ESCREVAL ("DIGITE O 1º NÚMERO: ")
   	LEIA (N1)
	ESCREVAL ("DIGITE O 2º NÚMERO: ")
   	LEIA (N2)
	ESCREVAL ("DIGITE O 3º NÚMERO: ")
   	LEIA (N3)
	SE N1 > N2 ENTAO
		MN := N1
	SENAO
		MN := N2
	FIMSE
	SE N3 > MN ENTAO
		MN := N3
	FIMSE
	SE (N1 = N2) E (N1 = N3) ENTAO
		ESCREVAL ("OS VALORES SÃO IGUAIS")
	SENAO
		ESCREVAL ("O NÚMERO MAIOR: ", MN)
	FIMSE
FIMALGORITMO
