ALGORITMO "EX5"
	VAR
		L, C : INTEIRO
		NUM : MATRIZ [1..10, 1..10] DE INTEIRO
INICIO
	PARA L DE 1 ATE 10 FACA
		PARA C DE 1 ATE 10 FACA
         		ESCREVAL ("DIGITE O NÚMERO: ")
         		LEIA (NUM[L,C])
			SE L = C ENTAO
				ESCREVAL ("NÚMERO DA DIAGONAL PRINCIPAL: ", NUM[L,C])
			FIMSE
		FIMPARA
	FIMPARA
FIMALGORITMO
